@using TennisBookings.Web.Core
@model TennisBookings.Web.Areas.Admin.ViewModels.BookingListerViewModel
@{
    ViewData["Title"] = "Upload Results";
}

<h1>@ViewData["Title"]</h1>

<div class="row">
    <section>
        <form method="post" enctype="multipart/form-data" asp-controller="Results" asp-action="FileUpload" asp-area="Admin">
            <div class="form-group">
                <div class="col-md-10">
                    <p>Upload a score CSV file using this form:</p>
                    <input type="file" id="file" name="file" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-10">
                    <input type="submit" value="Upload" />
                </div>
            </div>
        </form>
    </section>
</div>
